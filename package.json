{
  "name": "react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "deploy": "gh-pages -d dist -b gh-pages",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "preview": "vite preview",
    "format:fix": "prettier --write .",
    "test": "vitest",
