    "@testing-library/user-event": "^14.6.1",
    "clsx": "^2.1.1",
    "country-list": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "world-countries": "^5.1.0",
    "yup": "^1.7.0",
    "zustand": "^5.0.8"
